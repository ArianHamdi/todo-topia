    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\" --list-different",
    "prepare": "husky install",
    "type-check": "tsc --noEmit",
    "prisma-schema": "npx prisma db push --schema src/prisma/schema.prisma"
